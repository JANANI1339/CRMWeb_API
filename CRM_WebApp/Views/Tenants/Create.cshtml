@model Tenant

@{
    ViewData["Title"] = "Create";
}

<h1 class="mb-2 p-2">Create</h1>

<h4 class="mb-2 p-2">Tenant</h4>
<div class="row">
    <div class="container">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group mb-4">
                <label asp-for="TenantId" class="control-label" class="col-2"></label>
                <input asp-for="TenantId" class="form-control" class="col-4"/>
                <span asp-validation-for="TenantId" class="text-danger"></span>
            </div>
            <div class="form-group mb-4">
                <label asp-for="TenantName" class="control-label" class="col-2"></label>
                <input asp-for="TenantName" class="form-control" class="col-4" />
                <span asp-validation-for="TenantName" class="text-danger"></span>
            </div>
            <div class="form-group mb-4">
                <label asp-for="EmailId" class="control-label" class="col-2"></label>
                <input asp-for="EmailId" class="form-control" class="col-4" />
                <span asp-validation-for="EmailId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

